    "clean-deep": "^3.0.2",
    "colors": "^1.1.2",
    "compression": "^1.7.3",
    "cors": "^2.7.1",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "express": "^4.13.3",
    "forest-express-mongoose": "^2.16.1",
